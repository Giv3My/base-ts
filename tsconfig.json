{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "noImplicitAny": true,
    "baseUrl": "./src",
    "rootDir": "./src",
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}